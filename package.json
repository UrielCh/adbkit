{
    "name": "@u4/adbkit",
    "version": "4.1.11",
    "description": "A Typescript client for the Android Debug Bridge.",
    "main": "dist/cjs/index.js",
    "module": "dist/mjs/index.js",
    "types": "dist/mjs/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/mjs/index.js",
            "require": "./dist/cjs/index.js"
        }
    },
    "keywords": [
        "adb",
        "adbkit",
        "android",
        "logcat",
        "typescript",
        "monkey",
        "scrcpy"
    ],
    "bin": {
        "adbkit": "./bin/adbkit"
    },
    "bugs": {
        "url": "https://github.com/UrielCh/adbkit/issues"
    },
    "license": "Apache-2.0",
    "author": {
        "name": "openstf",
        "email": "contact@openstf.io",
        "url": "https://urielch.github.io/"
    },
    "contributors": [{
        "name": "uriel chemouni",
        "email": "uchemouni@gmail.com",
        "url": "https://urielch.github.io/"
    }],
    "repository": {
        "type": "git",
        "url": "https://github.com/UrielCh/adbkit.git"
    },
    "scripts": {
        "clean": "rimraf dist",
        "keycode": "ts-node tasks/keycode.ts",
        "servicemap": "ts-node tasks/servicemap.ts",
        "prepublish": "npm run clean && npm run build",
        "docs": "typedoc --entryPointStrategy expand",
        "build": "rimraf dist && tsc -p tsconfig.json && tsc -p tsconfig-cjs.json && npm run fixup",
        "fixup": "cp misc/cjs/package.json dist/cjs/ && cp misc/mjs/package.json dist/mjs/",
        "lint": "eslint   ./ --ext .ts",
        "format": "eslint ./ --ext .ts --fix",
        "test": "mocha -r ts-node/register --reporter spec --colors test/**/*.ts",
        "test2": "mocha -r ts-node/register --reporter spec --colors test/adb/thirdparty/**/*.ts",
        "test3": "mocha -r ts-node/register --reporter spec --colors test/adb/command/host-transport/serviceCall.ts"
    },
    "dependencies": {
        "@u4/adbkit-logcat": "2.1.2",
        "@u4/adbkit-monkey": "^1.0.5",
        "@u4/minicap-prebuilt": "^1.0.0",
        "commander": "9.4.1",
        "debug": "~4.3.4",
        "get-port": "6.1.2",
        "node-forge": "^1.3.1",
        "promise-duplex": "^6.0.0",
        "protobufjs": "^7.1.2",
        "xmldom": "^0.6.0",
        "xpath": "^0.0.32"
    },
    "devDependencies": {
        "@types/debug": "^4.1.7",
        "@types/mocha": "^10.0.0",
        "@types/node": "^18.8.4",
        "@types/node-forge": "^1.3.0",
        "@types/sinon": "^10.0.13",
        "@types/sinon-chai": "^3.2.8",
        "@types/xmldom": "^0.1.31",
        "@typescript-eslint/eslint-plugin": "^5.40.0",
        "@typescript-eslint/parser": "^5.40.0",
        "bench": "~0.3.6",
        "chai": "~4.3.6",
        "eslint": "^8.25.0",
        "mocha": "~10.0.0",
        "picocolors": "^1.0.0",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "sinon": "~14.0.1",
        "sinon-chai": "~3.7.0",
        "ts-node": "^10.9.1",
        "typedoc": "^0.23.16",
        "typedoc-plugin-rename-defaults": "^0.6.4",
        "typescript": "4.8.4",
        "why-is-node-running": "^2.2.2"
    },
    "engines": {
        "node": ">= 12.20.0",
        "npm": ">=7.0.0"
    },
    "files": [
        "dist",
        "bin"
    ],
    "optionalDependencies": {
        "@devicefarmer/minicap-prebuilt": "^2.7.1"
    }
}
