{
    "name": "@u4/adbkit",
    "version": "3.1.7",
    "description": "A Typescript client for the Android Debug Bridge.",
    "keywords": [
        "adb",
        "adbkit",
        "android",
        "logcat",
        "typescript",
        "monkey",
        "scrcpy"
    ],
    "bin": {
        "adbkit": "./bin/adbkit"
    },
    "bugs": {
        "url": "https://github.com/UrielCh/adbkit/issues"
    },
    "license": "Apache-2.0",
    "author": {
        "name": "Device Farmer",
        "email": "contact@openstf.io",
        "url": "https://urielch.github.io/"
    },
    "contributors": [
        {
            "name": "uriel chemouni",
            "email": "uchemouni@gmail.com",
            "url": "https://urielch.github.io/urielch/"
        }
    ],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/UrielCh/adbkit.git"
    },
    "scripts": {
        "clean": "rimraf dist",
        "keycode": "ts-node tasks/keycode.ts",
        "servicemap": "ts-node tasks/servicemap.ts",
        "prepublish": "npm run clean && npm run compile && npm run test",
        "compile": "tsc  -p .",
        "build": "tsc  -p .",
        "lint": "eslint   ./ --ext .ts",
        "format": "eslint ./ --ext .ts --fix",
        "test": "mocha -r ts-node/register --reporter spec --colors test/**/*.ts",
        "test2": "mocha -r ts-node/register --reporter spec --colors test/adb/thirdparty/**/*.ts"
    },
    "dependencies": {
        "@u4/adbkit-logcat": "2.1.1",
        "@u4/adbkit-monkey": "^1.0.3",
        "commander": "9.0.0",
        "debug": "~4.3.3",
        "install": "^0.13.0",
        "node-forge": "^1.2.1",
        "promise-duplex": "^6.0.0",
        "promise-socket": "^7.0.0",
        "protobufjs": "^6.11.2",
        "split": "~1.0.1"
    },
    "devDependencies": {
        "@types/debug": "^4.1.7",
        "@types/mocha": "^9.1.0",
        "@types/node": "^17.0.21",
        "@types/node-forge": "^1.0.0",
        "@types/sinon-chai": "^3.2.8",
        "@typescript-eslint/eslint-plugin": "^5.13.0",
        "@typescript-eslint/parser": "^5.13.0",
        "bench": "~0.3.6",
        "chai": "~4.3.6",
        "eslint": "^8.10.0",
        "mocha": "~9.2.1",
        "picocolors": "^1.0.0",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "sinon": "~13.0.1",
        "sinon-chai": "~3.7.0",
        "ts-node": "^10.6.0",
        "typescript": "4.5.5"
    },
    "engines": {
        "node": ">= 12.20.0"
    },
    "files": [
        "dist",
        "bin"
    ],
    "optionalDependencies": {
        "@devicefarmer/minicap-prebuilt": "^2.7.1"
    }
}
