{
    "name": "@u4/adbkit",
    "version": "4.0.0",
    "description": "A Typescript client for the Android Debug Bridge.",
    "keywords": [
        "adb",
        "adbkit",
        "android",
        "logcat",
        "typescript",
        "monkey",
        "scrcpy"
    ],
    "bin": {
        "adbkit": "./bin/adbkit"
    },
    "bugs": {
        "url": "https://github.com/UrielCh/adbkit/issues"
    },
    "license": "Apache-2.0",
    "author": {
        "name": "openstf",
        "email": "contact@openstf.io",
        "url": "https://urielch.github.io/"
    },
    "contributors": [
        {
            "name": "uriel chemouni",
            "email": "uchemouni@gmail.com",
            "url": "https://urielch.github.io/"
        }
    ],
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "repository": {
        "type": "git",
        "url": "https://github.com/UrielCh/adbkit.git"
    },
    "scripts": {
        "clean": "rimraf dist",
        "keycode": "ts-node tasks/keycode.ts",
        "servicemap": "ts-node tasks/servicemap.ts",
        "prepublish": "npm run clean && npm run compile && npm run docsUtil",
        "docs": "concurrently -m 1 npm:docs:*",
        "docs:Adb":     "documentation readme --quiet true --shallow --markdown-toc=false --readme-file docs/adb.md          --section API       ./src/adb.ts",
        "docs:Util":    "documentation readme --quiet true --shallow --markdown-toc=false --readme-file docs/adb.md          --section adb.util  ./src/adb/util.ts",
        "docs:Client":  "documentation readme --quiet true --shallow --markdown-toc=false --readme-file docs/Client.md       --section API       ./src/adb/client.ts",
        "docs:DClient": "documentation readme --quiet true --shallow --markdown-toc=false --readme-file docs/DeviceClient.md --section API       ./src/adb/DeviceClient.ts",
        "docs:PTr":     "documentation readme --quiet true --shallow --markdown-toc=false --readme-file docs/PushTransfer.md --section API       ./src/adb/sync/pushtransfer.ts",
        "docs:Sync":    "documentation readme --quiet true --shallow --markdown-toc=false --readme-file docs/Sync.md         --section API       ./src/adb/Sync.ts",
        "compile": "tsc -p .",
        "build": "tsc -p .",
        "lint": "eslint   ./ --ext .ts",
        "format": "eslint ./ --ext .ts --fix",
        "test": "mocha -r ts-node/register --reporter spec --colors test/**/*.ts",
        "test2": "mocha -r ts-node/register --reporter spec --colors test/adb/thirdparty/**/*.ts",
        "test3": "mocha -r ts-node/register --reporter spec --colors test/adb/command/host-transport/serviceCall.ts"
    },
    "dependencies": {
        "@u4/adbkit-logcat": "2.1.1",
        "@u4/adbkit-monkey": "^1.0.3",
        "commander": "9.3.0",
        "debug": "~4.3.4",
        "get-port": "^6.1.2",
        "install": "^0.13.0",
        "node-forge": "^1.3.1",
        "promise-duplex": "^6.0.0",
        "promise-socket": "^7.0.0",
        "protobufjs": "^6.11.3",
        "split": "~1.0.1",
        "xmldom": "^0.6.0",
        "xpath": "^0.0.32"
    },
    "devDependencies": {
        "@types/debug": "^4.1.7",
        "@types/mocha": "^9.1.1",
        "@types/node": "^18.0.0",
        "@types/node-forge": "^1.0.2",
        "@types/sinon": "^10.0.12",
        "@types/sinon-chai": "^3.2.8",
        "@types/xmldom": "^0.1.31",
        "@typescript-eslint/eslint-plugin": "^5.30.0",
        "@typescript-eslint/parser": "^5.30.0",
        "bench": "~0.3.6",
        "chai": "~4.3.6",
        "concurrently": "^7.2.2",
        "documentation": "^13.2.5",
        "eslint": "^8.18.0",
        "mocha": "~10.0.0",
        "picocolors": "^1.0.0",
        "prettier": "^2.7.1",
        "rimraf": "^3.0.2",
        "sinon": "~14.0.0",
        "sinon-chai": "~3.7.0",
        "ts-node": "^10.8.1",
        "typescript": "4.7.4"
    },
    "engines": {
        "node": ">= 12.20.0"
    },
    "files": [
        "dist",
        "bin"
    ],
    "optionalDependencies": {
        "@devicefarmer/minicap-prebuilt": "^2.7.1"
    }
}
